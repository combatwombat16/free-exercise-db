{
    "name": "free-exercise-db",
    "version": "1.0.0",
    "description": "Open Public Domain Exercise Dataset",
    "private": true,
    "scripts": {
        "start": "npm run build --prefix mcp && npm run start --prefix mcp",
        "mcp:build": "npm run build --prefix mcp",
        "mcp:start": "npm run start --prefix mcp",
        "site:dev": "npm run dev --prefix site",
        "site:build": "npm run build --prefix site",
        "lint": "make lint",
        "postinstall": "cd mcp && npm install && cd ../site && npm install"
    }
}